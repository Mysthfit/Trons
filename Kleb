task.spawn(function()
    while task.wait() do
        pcall(function()
          
             local Player = game:service"Players".LocalPlayer;
            if Player.Character.HumanoidRootPart:FindFirstChild("OverheadUI") then
                Player.Character.HumanoidRootPart.OverheadUI:Destroy()
            end
        end)
    end
end)






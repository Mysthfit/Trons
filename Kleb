   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
    local amountlp = 0

while amountlp and wait() < 21 do
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Islands["Enies Lobby"].Model.Spawner.thebossinnicaragua:WaitForChild("HumanoidRootPart").CFrame
